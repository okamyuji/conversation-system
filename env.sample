# Redis設定
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# アプリケーション設定
LOG_LEVEL=INFO
DATA_DIR=/app/data
BACKUP_ENABLED=true
BACKUP_INTERVAL=3600

# セキュリティ設定
REDIS_PASSWORD=
API_SECRET_KEY=your_secret_key_here_change_in_production

# Web UI設定
API_URL=http://localhost:8000
NODE_ENV=production

# バックアップ設定
BACKUP_RETENTION_DAYS=30
RDB_BACKUP_INTERVAL=6h
